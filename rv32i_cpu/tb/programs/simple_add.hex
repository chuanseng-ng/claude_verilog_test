// Simple addition test program
// Tests basic ALU operations and register file
//
// Program:
//   ADDI x1, x0, 10       ; x1 = 10
//   ADDI x2, x0, 20       ; x2 = 20
//   ADD  x3, x1, x2       ; x3 = x1 + x2 = 30
//   SUB  x4, x2, x1       ; x4 = x2 - x1 = 10
//   AND  x5, x1, x2       ; x5 = x1 & x2
//   OR   x6, x1, x2       ; x6 = x1 | x2
//   XOR  x7, x1, x2       ; x7 = x1 ^ x2
//   EBREAK                ; Halt CPU
//
// Expected results:
//   x1 = 10 (0x0000000A)
//   x2 = 20 (0x00000014)
//   x3 = 30 (0x0000001E)
//   x4 = 10 (0x0000000A)
//   x5 = 0  (0x00000000)  // 10 & 20 = 01010 & 10100 = 0
//   x6 = 30 (0x0000001E)  // 10 | 20 = 01010 | 10100 = 11110 = 30
//   x7 = 30 (0x0000001E)  // 10 ^ 20 = 01010 ^ 10100 = 11110 = 30

00A00093  // ADDI x1, x0, 10
01400113  // ADDI x2, x0, 20
002081B3  // ADD  x3, x1, x2
40208233  // SUB  x4, x2, x1
0020F2B3  // AND  x5, x1, x2
0020E333  // OR   x6, x1, x2
0020C3B3  // XOR  x7, x1, x2
00100073  // EBREAK
